{
  "name": "festivo-io/festivo-php",
  "description": "Official PHP SDK for Festivo Public Holidays API - Access 100+ countries' holiday data with UTC dates",
  "version": "0.1.11",
  "type": "library",
  "keywords": ["festivo", "holidays", "public-holidays", "api", "calendar", "php", "sdk"],
  "homepage": "https://getfestivo.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Festivo",
      "email": "support@getfestivo.com"
    }
  ],
  "support": {
    "email": "support@getfestivo.com",
    "source": "https://github.com/festivo-io/festivo-sdk",
    "issues": "https://github.com/festivo-io/festivo-sdk/issues"
  },
  "autoload": {
    "psr-4": {
      "Festivo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Festivo\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "guzzlehttp/promises": "^2.0"
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  }
}
